

<% if @symbol %>
  <% if @quote["Response"]%>
     <h4>That crypto symbol <%= @symbol %> does not exist. Please try again....<h4>
  <% else %>

<% @quote["DISPLAY"].each do |key, value|%>
<div class="jumbotron">
  <h1 class="display-4"><%= key %></h1>
       price: <%= value["USD"]["PRICE"]%></br>
      24hr high: <%= value["USD"]["HIGHDAY"]%></br>
      24hr low: <%= value["USD"]["LOWDAY"]%></br>
      markey cap: <%= value["USD"]["MKTCAP"]%></br>
</div>
    <% end %>
  <% end%> 
<% else %>
 <h4>Please enter a crypto symbol in the form above....<h4>
<% end %>